<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Sequences for construction.survey -->
        <record id="seq_sale_letter_guarantee" model="ir.sequence">
            <field name="name">L</field>
            <field name="code">initial.letter.guarantee</field>
            <field name="prefix">SLG/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_final_letter_guarantee" model="ir.sequence">
            <field name="name">L</field>
            <field name="code">final.letter.guarantee</field>
            <field name="prefix">PLG/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_initial_letter_guarantee_request" model="ir.sequence">
            <field name="name">L</field>
            <field name="code">initial.request.letter.guarantee</field>
            <field name="prefix">SLGR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_final_letter_guarantee_request" model="ir.sequence">
            <field name="name">L</field>
            <field name="code">final.request.letter.guarantee</field>
            <field name="prefix">PLGR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
